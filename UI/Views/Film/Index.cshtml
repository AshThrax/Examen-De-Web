@{
}

@model List<UI.Models.FilmViewModel>

<div>
    <table>
        <thead>
            <tr>
                <td>Id</td>
                <td>Titre</td>
                <td>Description</td>
                <td>Date</td>
            </tr>
        </thead>
        <Tbody>
            <tr> 
            @foreach(var film in Model)
            {
                <td>@film.Id</td>
                <td>@film.Titre</td>
                <td>@film.Description</td>
                <td>@film.Date</td>
            }</tr>
           
        </Tbody>
    </table>    
</div>